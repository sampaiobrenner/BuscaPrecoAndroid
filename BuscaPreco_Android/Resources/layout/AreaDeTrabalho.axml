        android:background="@drawable/button_logar_style"
        android:id="@+id/btnAreaDeTrabalho_SincronizarProdutos"
        android:layout_marginTop="12dp"
        android:drawableLeft="@drawable/sincronizar_48x48" />
    <Button
        android:text="Sincronizar UsuÃ¡rios"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/button_logar_style"
        android:id="@+id/btnAreaDeTrabalho_SincronizarUsuarios"
